{
  "name": "cms-generator",
  "displayName": "cms",
  "description": "code generate",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/BoxMeApp/cms-generator.git"
  },
  "publisher": "dzylikecode",
  "author": {
    "name": "Dz",
    "email": "mathBeWithCode@gmail.com"
  },
  "keywords": [
    "bloc",
    "flutter",
    "dart",
    "cms"
  ],
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cms-generator.folderContextAction",
        "title": "generate cms"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "cms-generator.folderContextAction",
          "when": "explorerResourceIsFolder == true",
          "group": "navigation@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
